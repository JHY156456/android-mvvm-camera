<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.example.mvvmappapplication.custom.HSUntactView
            style="@style/UntactTableOfLineStyle"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"/>

        <com.example.mvvmappapplication.custom.HSUntactEditText
            android:id="@+id/hsnetv_input"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/dimen_55"
            android:imeOptions="actionDone"
            android:inputType="text"
            android:textStyle="bold"
            android:maxLines="1"
            android:hint="직접입력해 주세요"
            android:textSize="@dimen/dimen_16"
            android:background="@null"/>
    </LinearLayout>
</FrameLayout>